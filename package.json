{
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "vite": "tsc && vite build",
    "serve": "vite preview",
    "start": "vite",
    "start:cli": "cross-env BROWSER=none vite --mode test",
    "build:asp": "npm i --legacy-peer-deps && npm run vite --verbose",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --silent --noStackTrace",
    "test:cov-open": "npm run test:cov && open coverage/lcov-report/index.html",
    "test:debug": "jest --detectOpenHandles",
    "test:clean": "jest --clearCache",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test start:cli http-get://localhost:5173 cy:open",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start:cli http-get://localhost:5173 cy:run",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "lint": "jest --config ./jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|less)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all check-types format check-format lint test:jest test:e2e:run build",
    "validate:setup": "npm-run-all check-types format check-format lint test:jest build",
    "setup": "npm i --legacy-peer-deps && npm run validate:setup"
  },
  "msw": {
    "workerDirectory": ""
  },
  "_moduleAliases": {
    "@babel/standalone": "node_modules/@babel/core",
    "babel-preset-env": "node_modules/@babel/preset-env"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "browserslist": ">0.75%, not ie 11, not UCAndroid >0, not OperaMini all",
  "dependencies": {
